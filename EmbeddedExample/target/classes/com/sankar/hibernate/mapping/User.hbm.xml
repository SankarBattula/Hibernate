<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.sankar.hibernate.entity.User" table="USER_TAB">
		<composite-id>
			<key-property name="userId" column="USER_ID"></key-property>
			<key-property name="userName" column="USER_NAME"></key-property>
		</composite-id>
			<property name="password" column="PASSWORD"></property>
			<property name="profession" column="PROFESSION"></property>
			<property name="city" column="CITY"></property>
	</class>
</hibernate-mapping>